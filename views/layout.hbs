                      <div class="showboy">
                        <button href="#apartment" data-toggle="tab">Apartment</button>

                        <button href="#house" data-toggle="tab">House</button>

                        <a href="rentRoom.html"><button>Room</button></a>

                        <a href="rentHostel.html"><button>Hostel</button></a>

                        <button href="#sale" data-toggle="tab">Property For Sale</button>
