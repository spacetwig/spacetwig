          <li class="cl-effect-1"><a data-scroll href="#privacy">Privacy</a></li>
        </ul>
      </nav>
    <div class="dropdown">
      {{#if user.profilePhoto}}
        <img src="{{user.profilePhoto}}" alt="photograph of potential user"  class="profile-photo dropdown-toggle" data-toggle="dropdown">
      {{else}}
        <img src="assets/img/default-avatar.png" alt="photograph of potential user"  class="profile-photo dropdown-toggle" data-toggle="dropdown">
      {{/if}}

      <ul class="dropdown-menu dropdown-menu-right">
            <li role="presentation"><a class="" data-toggle="modal" data-target="#myModal" role="menuitem" tabindex="-1" href="#">Add a Property</a></li>

            {{#if user}}
              <li role="presentation"><a class="" data-toggle="modal" data-target="#myModa2" role="menuitem" tabindex="-1" href="#">Edit Profile</a></li>

              <li role="presentation"><a class="" data-toggle="modal" data-target="#myModa2" role="menuitem" tabindex="-1" href="#">View Your Properties</a></li>
            {{else}}
              <li role="presentation"><a class="" data-toggle="modal" data-target="#myModa2" role="menuitem" tabindex="-1" href="#">Sign in</a></li>
            {{/if}}



            {{!--<li role="presentation"><a class="" data-toggle="modal" data-target="#myModal" role="menuitem" tabindex="-1" href="#">Sign up here</a></li>--}}

            <li role="presentation" class="divider"></li>
            {{#if user}}
              <li role="presentation"><a class="" role="menuitem" tabindex="-1" href="/users/signOut/signOutOfYourAccount">Logout</a></li>
            {{else}}
              <li role="presentation"><a class="" data-toggle="modal" data-target="#myModal" role="menuitem" tabindex="-1" href="#">Sign up </a></li>
            {{/if}}
      </ul>
    </div>

    </header><!--/.main-header-->

    {{#if errors}}
      {{#each errors}}
        <p class="alert alert-danger">{{msg}}</p>
      {{/each}}
    {{/if}}

    {{#if error}}
      <p class="alert alert-danger">{{error}}</p>
    {{/if}}

    {{#if success_msg}}
      <p class="alert alert-success">{{success_msg}}</p>
    {{/if}}

    {{#if error_msg}}
      <p class="alert alert-danger">{{error_msg}}</p>
    {{/if}}
  </div>

    {{{body}}}


    <!-- Modal Core -->
  <div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog">
      <div class="modal-content">
        <div class="modal-header">
          <div type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</div>
          Sign Up with Spacelist
        </div>
        <div class="modal-body">
          <!-- Tabs on Plain Card -->
              <div class="card card-nav-tabs card-plain">
                <div class="header header-danger">
                  <!-- colors: "header-primary", "header-info", "header-success", "header-warning", "header-danger" -->
                  <div class="nav-tabs-navigation">
                    <div class="nav-tabs-wrapper">
                    </div>
                  </div>
                </div>
                <div class="content">
                  <div class="tab-content text-center">
                    <div class="tab-pane active" id="signup">
                      <form method="post" action='/signupnewuser' enctype="multipart/form-data" >
                        <div class="social">
                          <a id="google_login" class="circle google" href="/auth/login/google_oauth2">
                              <i class="fa fa-google-plus fa-fw"></i>
                          </a>
                          <a id="facebook_login" class="circle facebook" href="/auth/login/facebook">
                              <i class="fa fa-facebook fa-fw"></i>
                          </a>
                        </div>
                        <div class="division">
                            <div class="line l"></div>
                              <span>or</span>
                            <div class="line r"></div>
                        </div>
                        <input placeholder="first name" type="text" required="true" name="firstName">
                        <input placeholder="last name" type="text" required="true" name="lastName">
                        <input placeholder="email"  required="" name="email">
                        <input placeholder="Password" type="password" minlength="6" id="password" name="password" required="required">
                        <input placeholder="Confirm Password" type="password" minlength="6" id="compare_password" required="required" name="confirmPassword">
                        <button href="#photo" data-toggle="tab">Next</button>
                    </div>
                    <div class="tab-pane" id="photo">
                      <div class="content">
                        <div class="picture-container">
                           <div class="picture">
                            <img src="assets/img/default-avatar.png" class="picture-src" id="wizardPicturePreview" title=""/>
                                 <input type="file" id="wizard-picture" name="file">
                           </div>
                        </div>
                      </div>

                      <div class="linebut">
                      <button href="#signup" data-toggle="tab">Back</button><button type="submit">Submit</button>
                      </div>

                      </form>
                    </div>
                  </div>
                </div>
              </div>
              <!-- End Tabs on plain Card -->
        </div>
      </div>
    </div>
  </div>

  <!-- Modal Core 2 -->
  <div class="modal fade" id="myModa2" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <div type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</div>
        Sign in with Spacelist
      </div>
      <div class="modal-body">
        <!-- Tabs on Plain Card -->
            <div class="card card-nav-tabs card-plain">
              <div class="header header-danger">
                <div class="nav-tabs-navigation">
                  <div class="nav-tabs-wrapper">
                  </div>
                </div>
              </div>
              <div class="content">
                <div class="tab-content text-center">
                  <div class="tab-pane active" id="signin">
                    <form method="post" action="/users/signIn/signInToYourAccount">
                      <div class="social">
                        <a id="google_login" class="circle google" href="/auth/google_oauth2">
                            <i class="fa fa-google-plus fa-fw"></i>
                        </a>
                        <a id="facebook_login" class="circle facebook" href="/auth/facebook">
                            <i class="fa fa-facebook fa-fw"></i>
                        </a>
                      </div>
                      <div class="division">
                          <div class="line l"></div>
                            <span>or</span>
                          <div class="line r"></div>
                      </div>
                      <input placeholder="Email" type="email" name="email" required="required">
                      <input placeholder="Password" type="password" id="password" name="password" required="required">
                      <div class="linebutt">
                        <button type="submit">Sign in</button>
                      </div>
                      <div class="forge">
                        <a href="#forgot" data-toggle="tab">Forgot password?</a>
                      </div>
                    </form>
                  </div>

                  <div class="tab-pane" id="forgot">
                    <form>
                      <p>Type in your Email to get a new password</p>
                      <input placeholder="Email" type="email" required="">
                      <div class="linebutt">
                        <button>Submit</button>
                      </div>
                      <div class="bak">
                        <a href="#signin" data-toggle="tab">< Back to Sign in</a>
                      </div>
                    </form>
                  </div>
                </div>
              </div>
            </div>
            <!-- End Tabs on plain Card -->
      </div>
    </div>
  </div>
  </div>

  <!--  modal core 2 ending    -->

  <!-- Modal Core 3 -->
  <div class="modal fade" id="myModa3" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
